{
  "mcpServers": {
    "playwright": {
      "command": "npx",
      "args": ["@anthropic/mcp-server-playwright"]
    },
    "github": {
      "command": "npx",
      "args": ["@anthropic/mcp-server-github"],
      "env": {
        "GITHUB_TOKEN": "${GITHUB_TOKEN}"
      }
    },
    "puppeteer": {
      "command": "npx",
      "args": ["@anthropic/mcp-server-puppeteer"]
    },
    "fetch": {
      "command": "npx",
      "args": ["@anthropic/mcp-server-fetch"]
    }
  }
}
